<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".accounts">


    <ImageView
        android:layout_width="90dp"
        android:layout_height="106dp"
        android:src="@drawable/logo"
        app:layout_constraintBottom_toBottomOf="@+id/textView18"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.049"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.595" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView1"
        android:layout_width="409dp"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.463" >

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView2"
        android:layout_width="409dp"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.787" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Account"
        android:textColor="@color/black"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.551"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.095" />

    <LinearLayout
        android:id="@+id/linearLayout_transactionhistory"
        android:layout_width="match_parent"
        android:layout_height="71dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.363">

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="76dp"
            android:layout_height="56dp"
            app:srcCompat="@drawable/ic_baseline_history_24"
            tools:layout_editor_absoluteX="61dp"
            tools:layout_editor_absoluteY="510dp" />


        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:paddingTop="14dp"
            android:text="Transaction History"
            android:textAlignment="center"

            android:textColor="@color/black"
            android:textSize="28sp" />

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="67dp"
            android:layout_height="72dp"
            android:layout_marginLeft="29dp"
            app:srcCompat="@drawable/ic_baseline_arrow_right_24" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout_curreny"
        android:layout_width="match_parent"
        android:layout_height="71dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.481">

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="76dp"
            android:layout_height="56dp"
            app:srcCompat="@drawable/ic_baseline_attach_money_24"
            tools:layout_editor_absoluteX="61dp"
            tools:layout_editor_absoluteY="510dp" />


        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:paddingTop="14dp"
            android:text="Currency"
            android:textAlignment="center"

            android:textColor="@color/black"
            android:textSize="28sp" />

        <ImageView
            android:id="@+id/imageView8"
            android:layout_width="67dp"
            android:layout_height="72dp"
            android:layout_marginLeft="161dp"
            app:srcCompat="@drawable/ic_baseline_arrow_right_24" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout_wallet"
        android:layout_width="match_parent"
        android:layout_height="71dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.256">

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="76dp"
            android:layout_height="56dp"
            app:srcCompat="@drawable/ic_baseline_account_balance_wallet_24"
            tools:layout_editor_absoluteX="61dp"
            tools:layout_editor_absoluteY="510dp" />


        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingTop="14dp"
            android:text="Wallet"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="28sp" />

        <ImageView
            android:id="@+id/imageView11"
            android:layout_width="167dp"
            android:layout_height="72dp"
            android:layout_marginLeft="150dp"
            app:srcCompat="@drawable/ic_baseline_arrow_right_24" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout_help_center"
        android:layout_width="match_parent"
        android:layout_height="71dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.625">

        <ImageView
            android:id="@+id/imageView12"
            android:layout_width="76dp"
            android:layout_height="56dp"
            app:srcCompat="@drawable/ic_baseline_help_24"
            tools:layout_editor_absoluteX="61dp"
            tools:layout_editor_absoluteY="510dp" />


        <TextView
            android:id="@+id/textView13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:paddingTop="14dp"
            android:text="Help Center"
            android:textAlignment="center"

            android:textColor="@color/black"
            android:textSize="28sp" />

        <ImageView
            android:id="@+id/imageView14"
            android:layout_width="67dp"
            android:layout_height="72dp"
            android:layout_marginLeft="125dp"
            app:srcCompat="@drawable/ic_baseline_arrow_right_24" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout_privacypolicy"
        android:layout_width="match_parent"
        android:layout_height="71dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.745">

        <ImageView
            android:id="@+id/imageView15"
            android:layout_width="76dp"
            android:layout_height="56dp"
            app:srcCompat="@drawable/ic_baseline_privacy_tip_24"
            tools:layout_editor_absoluteX="61dp"
            tools:layout_editor_absoluteY="510dp" />


        <TextView
            android:id="@+id/textView16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:paddingTop="14dp"
            android:text="Privacy Policy"
            android:textAlignment="center"

            android:textColor="@color/black"
            android:textSize="28sp"
            tools:ignore="DuplicateIds" />

        <ImageView
            android:id="@+id/imageView17"
            android:layout_width="67dp"
            android:layout_height="72dp"
            android:layout_marginLeft="95dp"
            app:srcCompat="@drawable/ic_baseline_arrow_right_24"
            tools:ignore="DuplicateIds" />
    </LinearLayout>

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Accounts"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.088"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.189" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView"
        android:layout_width="409dp"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.109" />


</androidx.constraintlayout.widget.ConstraintLayout>
